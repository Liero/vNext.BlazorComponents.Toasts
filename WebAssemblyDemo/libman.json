{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.bundle.js",
        "css/bootstrap.min.css"
      ]
    },
    {
      "library": "font-awesome@5.15.4",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css",
        "webfonts/fa-solid-900.woff2",
        "webfonts/fa-solid-900.woff",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-regular-400.woff",
        "webfonts/fa-brands-400.woff2",
        "webfonts/fa-brands-400.woff"
      ]
    },
    {
      "library": "open-iconic@1.1.1",
      "destination": "wwwroot/lib/open-iconic/",
      "files": [
        "font/css/open-iconic-bootstrap.min.css",
        "font/fonts/open-iconic.woff" 
      ]
    },
    {
      "library": "bootstrap-icons@1.7.2",
      "destination": "wwwroot/lib/bootstrap-icons/",
      "files": [
        "font/bootstrap-icons.min.css",
        "font/fonts/bootstrap-icons.woff2",
        "font/fonts/bootstrap-icons.woff"
      ]
    }
  ]
}